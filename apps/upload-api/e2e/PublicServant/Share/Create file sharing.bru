meta {
  name: Create file sharing
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/api/v1/metadata/share
  body: formUrlEncoded
  auth: none
}

headers {
  Authorization: Bearer {{accessToken}}
}

body:form-urlencoded {
  fileId: {{fileId}}
  userId: user-id
}

assert {
  res.status: eq 201
  res.body.data.userId: eq user-id
}
