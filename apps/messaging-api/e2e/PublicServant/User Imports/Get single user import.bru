meta {
  name: Get single user import
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/api/v1/user-imports/{{userImportId}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

vars:post-response {
  usersData: res.body.data.usersData
}

assert {
  res.status: eq 200
  res.body.data: isDefined
  res.body.data.id: isDefined
}
