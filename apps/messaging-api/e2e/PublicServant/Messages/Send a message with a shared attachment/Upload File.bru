meta {
  name: Upload File
  type: http
  seq: 1
}

post {
  url: {{uploadBaseUrl}}/api/v1/files
  body: multipartForm
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

body:multipart-form {
  file: @file(dummy-attachment.txt)
}

vars:post-response {
  attachmentId: res.body.data.id
}

assert {
  res.status: eq 201
  res.body.data.id: isString
}
