meta {
  name: Share File With User
  type: http
  seq: 2
}

post {
  url: {{uploadBaseUrl}}/api/v1/metadata/share
  body: json
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

body:json {
  {
    "fileId": "{{attachmentId}}",
    "userId": "{{consentUserId}}"
  }
}

assert {
  res.status: eq 201
  res.body.data.fileId: isString
}
