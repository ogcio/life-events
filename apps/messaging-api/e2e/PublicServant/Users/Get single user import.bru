meta {
  name: Get single user import
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/api/v1/users/{{userId}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

vars:post-response {
  ~userImportId: res.body.data[0].importId
}

assert {
  res.status: eq 200
  res.body.data: isDefined
  res.body.data.id: isDefined
}
