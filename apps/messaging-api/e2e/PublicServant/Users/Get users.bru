meta {
  name: Get users
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/api/v1/users?activeOnly=true
  body: none
  auth: bearer
}

query {
  activeOnly: true
}

auth:bearer {
  token: {{accessToken}}
}

vars:post-response {
  userId: res.body.data[0].userId
}

assert {
  res.status: eq 200
  res.body.data: isDefined
  res.body.data[0]: isDefined
  res.body.data[0].id: isDefined
}
