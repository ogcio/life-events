meta {
  name: Get non-activeOnly users
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/api/v1/users?activeOnly=false
  body: none
  auth: bearer
}

query {
  activeOnly: false
}

auth:bearer {
  token: {{accessToken}}
}

vars:post-response {
  nonActiveUserId: res.body.data && res.body.data[0] ?  res.body.data[0].userId : undefined
}

assert {
  res.status: eq 200
  res.body.data: isDefined
  res.body.data[0]: isDefined
  res.body.data[0].id: isDefined
}
