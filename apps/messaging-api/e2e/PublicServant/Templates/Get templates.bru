meta {
  name: Get templates
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/api/v1/templates
  body: none
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

vars:post-response {
  templateId: res.body.data[0].id
}

assert {
  res.status: eq 200
  res.body.data: isDefined
  res.body.data[0]: isDefined
  res.body.data[0].id: isDefined
  res.body.data[0].contents: isDefined
}
